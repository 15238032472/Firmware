#!nsh
#
# Atmel SAM E70 Xplained specific board init
#
#------------------------------------------------------------------------------
#

if [ $AUTOCNF = yes ]
then

fi

# Start sensor drivers if not in HIL mode
if param compare SYS_HITL 0
then
	# External I2C bus
	hmc5883 -C -T -X start

	# Internal SPI bus mpu9250 is rotated 90 deg yaw
	mpu9250 -R 2 start
fi
